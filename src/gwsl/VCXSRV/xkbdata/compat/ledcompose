
default partial xkb_compatibility "compose"  {

    virtual_modifiers   Compose;

    interpret Multi_key+Any {
	virtualModifier= Compose;
	action = LatchMods(modifiers=modMapMods);
    };

   indicator "Compose" {
       allowExplicit;
       whichModState= Latched;
       modifiers= Compose;
   };
};
