xkb_keycodes "lk_common" {
    // "Function" keys
    <FK01>	= 86;
    <FK02>	= 87;
    <FK03>	= 88;
    <FK04>	= 89;
    <FK05>	= 90;
    <FK06>	= 100;
    <FK07>	= 101;
    <FK08>	= 102;
    <FK09>	= 103;
    <FK10>	= 104;
    <FK11>	= 113;
    <FK12>	= 114;

    <UP>	= 170;
    <LEFT>	= 167;
    <DOWN>	= 169;
    <RGHT>	= 168;

    // "Keypad" keys
    <KP7>	= 157;
    <KP8>	= 158;
    <KP9>	= 159;
    <KP4>	= 153;
    <KP5>	= 154;
    <KP6>	= 155;
    <KP1>	= 150;
    <KP2>	= 151;
    <KP3>	= 152;
    <KPEN>	= 149;
    <KP0>	= 146;
    <KPDL>	= 148;

    // "Alphanumeric" keys
    <AE00>	= 191;
    <AE01>	= 192;
    <AE02>	= 197;
    <AE03>	= 203;
    <AE04>	= 208;
    <AE05>	= 214;
    <AE06>	= 219;
    <AE07>	= 224;
    <AE08>	= 229;
    <AE09>	= 234;
    <AE10>	= 239;
    <AE11>	= 249;
    <AE12>	= 245;
    <BKSP>	= 188;

    <TAB>	= 190;
    <AD01>	= 193;
    <AD02>	= 198;
    <AD03>	= 204;
    <AD04>	= 209;
    <AD05>	= 215;
    <AD06>	= 220;
    <AD07>	= 225;
    <AD08>	= 230;
    <AD09>	= 235;
    <AD10>	= 240;
    <AD11>	= 250;
    <AD12>	= 246;
    <RTRN>	= 189;

    <LCTL>	= 175;
    <CAPS>	= 176;
    <AC01>	= 194;
    <AC02>	= 199;
    <AC03>	= 205;
    <AC04>	= 210;
    <AC05>	= 216;
    <AC06>	= 221;
    <AC07>	= 226;
    <AC08>	= 231;
    <AC09>	= 236;
