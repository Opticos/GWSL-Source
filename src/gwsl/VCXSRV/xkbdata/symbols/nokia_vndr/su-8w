    // XXX Not entirely sure if fourth level is quotedbl or diaeresis.
    //     The former makes a lot more sense.
    key <AC11> { [ dead_acute,   dead_diaeresis, apostrophe,           quotedbl        ] };

    key <AB01> { [ z,            Z,              ae,                   AE              ] };
    key <AB02> { [ x,            X                                                     ] };
