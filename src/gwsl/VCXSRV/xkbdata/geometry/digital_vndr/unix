        row { top = 21;	left = 7;
	    keys {  { <TAB>, "LONG1" },
		    <AD01>, <AD02>, <AD03>, <AD04>, <AD05>, <AD06>, 
		    <AD07>, <AD08>, <AD09>, <AD10>, <AD11>, <AD12>, 
		    { <RTRN>, "RTRN" }
	    };
