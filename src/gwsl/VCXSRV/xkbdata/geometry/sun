    shape "TABK" { { [ 27,18] }, { [2,1], [25,17] } };
    shape "KANJ" { { [ 27,18] }, { [2,1], [25,17] } };
    shape "EXEC" { { [ 27,18] }, { [2,1], [25,17] } };
    shape "RTRN" { 
        approx = { [ 0, 0], [28,37] },
        { [ 0, 0], [28, 0], [28,37],
          [ 5,37], [ 5,18], [ 0,18] },
