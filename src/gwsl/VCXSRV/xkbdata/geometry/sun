	    };
	};
	row {
	    top= 77;
	    keys { <FIND>, <CUT>, { <LCTL>, 9, shape="LCTL" },
		   <LALT>, {<LMTA>, "LMTA"},
		 { <SPCE>, "SPCE" },
		   {<RMTA>, "RMTA"}, <COMP>, <ALGR>
	    };
	};
    }; // End of "Alpha" section

    section "Editing" {
