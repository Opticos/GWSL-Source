		    keys { {<LCTL>, "LCTL"}, <FN>, <LWIN>, <LALT>, {<SPCE>, "SPCE"},
			   <RALT>, <MENU>, {<RCTL>, "RCTL"}
		    };
		};
	};

	section "Editing" {
		top = 16.5;
		left = 289.9;
		key.shape = "EDIT";
		key.gap = 1.4;
		row {
		    top = 0;
		    vertical = true;
		    keys {
				<HOME>, <PGUP>, <PGDN>, <END>
		    };
		};
	};

	section "Arrows" {
		top = 76.4;
		key.shape = "ARRW";
		left = 249.3;
		row {
			top = 0;
			left = 19.1;
			keys {
				<UP>
			};
		};
		row {
			top = 19.5;
			left = 0;
			keys {
				<LEFT>, <DOWN>, <RGHT>
			};
		};
	};
};
