		    { <FK11>, 6 }, <FK12>, <FK13>, <FK14>, <FK15>
	    };
	};
    }; // End of "Function" section

    section "Alpha" {
	top= 54;
	row {
	    top= 1;
	    keys { { <ESC>, shape="BKSP"}, 
		   <AE01>, <AE02>, <AE03>, <AE04>,
		   <AE05>, <AE06>, <AE07>, <AE08>, <AE09>,
		   <AE10>, <AE11>, <AE12>, <BKSL>,
		   { <BKSP>, "BKSP" }
	    };
	};
	row {
	    top= 20;
