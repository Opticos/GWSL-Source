		    <AB06>, <AB07>, <AB08>, <AB09>, <AB10>, <AB11>,
		    { <RTSH>, "RTSH" }
	    };
	};
	row {
	    top= 77;
	    keys { { <ALGR>, 35 } , <LALT>, { <NFER>, "FUNC" },
		   { <SPCE>, "SPCE" }, { <XFER>, "FUNC" }
		   
	    };
	};
    }; // End of "Alpha" section

    section "Editing" {
	top= 54;
	left= 281;
	row {
	    top= 1;
	    keys { <INS>, <DELE> };
	};
        row {
	    top= 20;
